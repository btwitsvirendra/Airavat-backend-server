{
  "name": "airavat-b2b-marketplace",
  "version": "1.0.0",
  "description": "Enterprise B2B Marketplace Backend for Indian and UAE Businesses - Comprehensive B2B Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "test": "NODE_ENV=test jest --coverage --runInBand",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:unit": "NODE_ENV=test jest tests/unit --coverage",
    "test:integration": "NODE_ENV=test jest tests/integration --runInBand",
    "test:e2e": "NODE_ENV=test jest tests/e2e --runInBand",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "worker:start": "node src/jobs/worker.js",
    "build": "npm run db:generate",
    "docker:build": "docker build -t airavat-api .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docs:generate": "node scripts/generate-docs.js",
    "health": "curl http://localhost:3000/health",
    "prepare": "npm run db:generate"
  },
  "keywords": [
    "b2b",
    "marketplace",
    "ecommerce",
    "india",
    "uae",
    "wholesale",
    "manufacturing",
    "gst",
    "vat",
    "e-invoice"
  ],
  "author": "Virendra",
  "license": "PROPRIETARY",
